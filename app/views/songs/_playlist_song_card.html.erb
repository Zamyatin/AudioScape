<div class="songcard">
	<span>
	<div class="coverart">
		<a href=<%= groove_session.get_song_url_by_id(song.link) %> >
			<img src=<%= song.coverart %> />
		</a>
	</div>
	<div class="songinfo">
		<h3><%= song.artist %> - <%= song.title %><h3>
	</div>
	<%= button_to "Delete Song", [@playlist, song], method: :delete, :class => 'button tiny' %>
	</span>
</div>
